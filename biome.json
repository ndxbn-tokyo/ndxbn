{
	"$schema": "https://biomejs.dev/schemas/2.2.2/schema.json",
	"linter": {
		"rules": {
			"all": true,
			"suspicious": {
				"noConsole": "off",
				"noConsoleLog": "off"
			}
		}
	},
	"files": {
		"ignoreUnknown": true,
		"ignore": ["out/"]
	},
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true,
		"defaultBranch": "main"
	}
}
